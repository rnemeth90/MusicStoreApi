services:
  musicapi:
    build:
      context: C:\repos\MusicApi
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MusicApi
      target: base
    container_name: MusicApi
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: musicapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/MusicApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\repos\MusicApi:/app:rw
    - C:\Users\Ryan.Nemeth\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Ryan.Nemeth\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Ryan.Nemeth\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Ryan.Nemeth\.nuget\packages:/root/.nuget/packages:ro
    - C:\repos\MusicApi:/src:rw
version: '3.4'