services:
  musicapi:
    build:
      context: C:\repos\MusicApi
      dockerfile: Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: musicapi
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\Ryan.Nemeth\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\Ryan.Nemeth\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
version: '3.4'